{
  "name": "fundamental-react",
  "version": "0.4.1-rc.5",
  "private": false,
  "description": "SAP Fiori Fundamentals, implemented in React",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "homepage": "https://sap.github.io/fundamental-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/fundamental-react"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-doc": "node scripts/build.js && cp build/index.html build/404.html",
    "build-js": "node scripts/build.js",
    "build:lint:fix": "eslint --fix 'src/**' --ext .js,.jsx --env browser,node",
    "build:lint": "eslint 'src/**' --ext .js,.jsx --env browser,node",
    "build": "npm run build:index && rm -rf lib && NODE_ENV=production babel src --out-dir lib --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\",\"src/**/*.Component.js\",\"src/_playground/*\"",
    "build:index": "babel-node devtools/buildIndexFiles.js",
    "config:lint": "eslint 'config/**' --ext .js,.jsx --env browser,node",
    "config:lint:fix": "eslint --fix 'config/**' --ext .js,.jsx --env browser,node",
    "deploy": "gh-pages -d build",
    "devtools:lint": "eslint 'devtools/**' --ext .js,.jsx --env browser,node",
    "lint:fix": "npm run build:lint:fix  && npm run scripts:lint:fix",
    "lint:pre-commit": "printf \"running pre-commit lint...\"  && npm run lint && printf \"done!\n\"",
    "lint": "npm run build:lint && npm run scripts:lint && npm run config:lint && npm run devtools:lint",
    "release": "./scripts/publish-release.sh",
    "release:create": "create-release",
    "serve": "BUILD_ENV=development npm run build-doc && serve -s build",
    "scripts:lint": "eslint 'scripts/**' --ext .js,.jsx --env browser,node",
    "scripts:lint:fix": "eslint --fix 'scripts/**' --ext .js,.jsx --env browser,node",
    "size": "npm run build && size-limit",
    "size:debug": "npm run build && size-limit --why",
    "start": "npm run build && node scripts/start.js",
    "start:playground": "FUNDAMENTAL_REACT_PLAYGROUND=true npm start",
    "std-version": "standard-version -m \"chore(release): version %s build ${TRAVIS_BUILD_NUMBER} [ci skip]\"",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || echo -e \"Coveralls failed.\"",
    "test:dev": "jest",
    "test:watch": "jest --watch",
    "test": "npm run test:watch",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "focus-trap-react": "^6.0.0",
    "prop-types": "^15.7.1",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "7.1.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/polyfill": "^7.1.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "2.4.1",
    "annotated-prop-types": "^0.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-plugin-named-asset-import": "^0.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.23",
    "babel-preset-react-app": "^6.1.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "coveralls": "^3.0.2",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "5.6.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.1",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-ie11": "^1.0.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-loosely-restrict-imports": "^0.1.15",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "fiori-fundamentals": "^1.4.3",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.0",
    "gh-pages": "^2.0.1",
    "github-assistant": "^0.2.0",
    "group-array": "^0.3.3",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.1",
    "jest-resolve": "23.6.0",
    "mini-css-extract-plugin": "0.4.3",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "pre-commit": "^1.2.2",
    "react": "^16.6.3",
    "react-app-polyfill": "^0.1.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-markdown": "^4.0.6",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-syntax-highlighter": "^9.0.1",
    "react-test-renderer": "^16.6.3",
    "resolve": "1.8.1",
    "sass-loader": "7.1.0",
    "serve": "^10.1.2",
    "size-limit": "^0.21.1",
    "sort-by": "^1.2.0",
    "standard-version": "^4.4.0",
    "style-loader": "0.23.0",
    "terser-webpack-plugin": "1.1.0",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "peerDependencies": {
    "fiori-fundamentals": "^1.4.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "pre-commit": {
    "colors": true,
    "run": [
      "lint:pre-commit"
    ],
    "silent": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/{App,Routes,index,registerServiceWorker}.js",
      "!src/**/*.Component.js",
      "!src/_playground/**"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "./config/jest/jest.config.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  }
}
