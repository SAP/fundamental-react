{
  "name": "fundamental-react",
  "version": "0.7.0-rc.1",
  "private": false,
  "description": "SAP Fundamentals, implemented in React",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "homepage": "https://sap.github.io/fundamental-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/fundamental-react"
  },
  "scripts": {
    "build:copy-files": "cp -rf package.json README.md NOTICE.txt LICENSE.txt CHANGELOG.md lib",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-doc": "node scripts/build.js && cp build/index.html build/404.html",
    "build-doc:netlify": "npm run build && NETLIFY=true npm run build-doc",
    "build-js": "node scripts/build.js",
    "build:lint:fix": "npm run build:lint -- --fix",
    "build:lint": "eslint 'src/**' --ext .js,.jsx --env browser,node",
    "build": "npm run build:index && rm -rf lib && NODE_ENV=production babel src --out-dir lib --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\",\"src/**/*.Component.js\",\"src/_playground/*\"",
    "build:index": "babel-node devtools/buildIndexFiles.js",
    "config:lint": "eslint 'config/**' --ext .js,.jsx --env browser,node",
    "config:lint:fix": "npm run config:lint -- --fix",
    "deploy": "gh-pages -d build",
    "devtools:lint": "eslint 'devtools/**' --ext .js,.jsx --env browser,node",
    "lint:fix": "npm run build:lint:fix  && npm run scripts:lint:fix && npm run config:lint:fix",
    "lint:pre-commit": "printf \"running pre-commit lint...\"  && npm run lint && printf \"done!\n\"",
    "lint": "npm run build:lint && npm run scripts:lint && npm run config:lint && npm run devtools:lint && npm run style:lint",
    "release": "./scripts/publish-release.sh",
    "release:create": "create-release",
    "serve": "BUILD_ENV=development npm run build-doc && serve -s build",
    "scripts:lint": "eslint 'scripts/**' --ext .js,.jsx --env browser,node",
    "scripts:lint:fix": "npm run scripts:lint -- --fix",
    "size": "npm run build && size-limit",
    "size:debug": "DEBUG=true npm run size",
    "start": "npm run build && node scripts/start.js",
    "start:playground": "FUNDAMENTAL_REACT_PLAYGROUND=true npm start",
    "std-version": "standard-version -m \"chore(release): version %s build ${TRAVIS_BUILD_NUMBER} [ci skip]\"",
    "style:lint": "stylelint 'src/**/*.scss' 'src/**/*.css'",
    "test:coverage": "npm run test:dev -- --coverage",
    "test:coverage:watch": "npm run test:coverage -- --watch",
    "test:coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js || echo -e \"Coveralls failed.\"",
    "test:dev": "jest --config=./config/jest/jest.config.js",
    "test:watch": "npm run test:dev -- --watch",
    "test": "npm run test:watch",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "chain-function": "^1.0.1",
    "classnames": "^2.2.6",
    "focus-trap-react": "^6.0.0",
    "fundamental-styles": "~0.2.0-rc.12",
    "keycode": "^2.2.0",
    "prop-types": "^15.7.1",
    "react-foco": "^1.3.0",
    "react-overlays": "^1.1.2",
    "react-popper": "^1.3.3",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "7.4.5",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-react-inline-elements": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "annotated-prop-types": "^0.3.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.23",
    "babel-preset-react-app": "^9.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "compression-webpack-plugin": "^3.0.0",
    "core-js": "^3.1.4",
    "coveralls": "^3.0.2",
    "css-loader": "3.0.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "5.16.0",
    "eslint-config-react-app": "^3.0.5",
    "eslint-loader": "2.1.2",
    "eslint-plugin-compat": "^2.6.3",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-ie11": "^1.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-loosely-restrict-imports": "^0.1.15",
    "eslint-plugin-react": "7.14.2",
    "file-loader": "4.0.0",
    "fs-extra": "8.0.1",
    "gh-pages": "^2.0.1",
    "github-assistant": "^0.3.0",
    "group-array": "^0.3.3",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "jest": "24.8.0",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "postcss-clean": "^1.1.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "pre-commit": "^1.2.2",
    "react": "^16.6.3",
    "react-dev-utils": "^6.1.1",
    "react-dom": "^16.6.3",
    "react-element-to-jsx-string": "^14.0.2",
    "react-helmet-async": "^1.0.2",
    "react-markdown": "^4.0.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-syntax-highlighter": "^11.0.1",
    "react-test-renderer": "^16.6.3",
    "regenerator-runtime": "^0.13.2",
    "resolve": "1.11.1",
    "sass-loader": "7.1.0",
    "serve": "^10.1.2",
    "size-limit": "^1.3.6",
    "sort-by": "^1.2.0",
    "standard-version": "^4.4.0",
    "style-loader": "0.23.1",
    "stylelint": "^10.1.0",
    "terser-webpack-plugin": "1.3.0",
    "tocbot": "^4.5.0",
    "url-loader": "2.0.1",
    "webpack": "4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-server": "^3.1.11",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "peerDependencies": {
    "react": "^16.6.3",
    "react-dom": "^16.6.3"
  },
  "pre-commit": {
    "colors": true,
    "run": [
      "lint:pre-commit"
    ],
    "silent": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
